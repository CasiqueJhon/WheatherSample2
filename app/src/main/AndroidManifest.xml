<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jhonisaac.wheathersample2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="Wheather.MinutelyActivityWheather">
        </activity>
        <activity
            android:name="Wheather.HourlyActivityWheather"
            android:parentActivityName="Wheather.WheatherActivity">
        </activity>
        <activity
            android:name="Wheather.DailyActivityWheather"
            android:parentActivityName="Wheather.WheatherActivity">
        </activity>
        <activity android:name="Wheather.WheatherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>